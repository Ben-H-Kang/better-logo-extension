{
    "manifest_version": 3,
    "name": "Refer.me with actual logo",
    "description": "this is the way",
    "version": "0.0.1",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
          "js": ["contentScript.js"],
          "matches": [
            "https://www.refer.me/",
            "https://www.refer.me/*"         
          ]
        }
    ]
}